@model ReportChartViewModel
@{
    ViewData["Title"] = "لوحة التحكم";
}

<div class="row mt-3">

    <div class="col-md-3 ">
        <div class="card  mb-3">
            <div class="card-body">
                <div class="row">
                    <div class="col-8">
                        <p class="card-text">Users</p>
                        <h5 class="card-title">@ViewBag.usersCount</h5>
                    </div>
                    <div class="col-4 d-flex align-items-center">
                        <i class="bi bi-people fs-1 text-secondary"></i>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="col-md-3 ">
        <div class="card  mb-3">
            <div class="card-body">
                <div class="row">
                    <div class="col-8">
                        <p class="card-text">Active Users</p>
                        <h5 class="card-title">@ViewBag.ActiveUsers</h5>
                    </div>
                    <div class="col-4 d-flex align-items-center">
                        <i class="bi bi-person-video3 fs-1 text-secondary"></i>
                    </div>
                </div>
            </div>
        </div>
    </div>
   
</div>
@section scripts {

}
