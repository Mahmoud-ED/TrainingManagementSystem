        <!-- Topbar -->
        <div class="topbar">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 col-md-5 col-12">
                    </div>
                </div>
            </div>
        </div>
        <!-- End Topbar -->
        <!-- Header Inner -->
        <div class="header-inner">
            <div class="container">
                <div class="inner">
                    <div class="row">
                        <div class="col-lg-3 col-md-3 col-12">
                            <!-- Start Logo -->
                            <div class="logo">
                                <a href="index.html"> <img src="~/img/oo.png" />  </a>
                            </div>
                            <!-- End Logo -->
                            <!-- Mobile Nav -->
                            <div class="mobile-nav">
                                <div class="slicknav_menu">
                                    @* <a href="#" aria-haspopup="true" role="button" tabindex="0" class="slicknav_btn slicknav_collapsed" style="outline: none;"><span class="slicknav_menutxt"></span><span class="slicknav_icon slicknav_no-text"><span class="slicknav_icon-bar"></span><span class="slicknav_icon-bar"></span><span class="slicknav_icon-bar"></span></span></a> *@<ul class="slicknav_nav slicknav_hidden" aria-hidden="true" role="menu" style="display: none;">
                                        <li class="active slicknav_collapsed slicknav_parent">
                                            <a href="#" role="menuitem" aria-haspopup="true" tabindex="-1" class="slicknav_item slicknav_row" style="outline: none;">
                                                <a href="#" tabindex="-1">الرئيسية <i class="icofont-rounded-down"></i></a>
                                                <span class="slicknav_arrow">►</span>
                                            </a>

                                        </li>
                                        <li class="slicknav_collapsed slicknav_parent">
                                            <a href="#" role="menuitem" aria-haspopup="true" tabindex="-1" class="slicknav_item slicknav_row" style="outline: none;">
                                                <a href="#" tabindex="-1">الدورات التدريبية <i class="icofont-rounded-down"></i></a>
                                                <span class="slicknav_arrow">►</span>
                                            </a>
                                            <ul class="dropdown slicknav_hidden" role="menu" aria-hidden="true" style="display: none;">
                                                <li><a href="doctors.html" role="menuitem" tabindex="-1">التخصصات</a></li>
                                                <li>
                                                    <a href="doctor-details.html" role="menuitem" tabindex="-1">Doctor Details</a>
                                                </li>
                                            </ul>
                                        </li>
                                        <li class="slicknav_collapsed slicknav_parent">
                                            <a href="#" role="menuitem" aria-haspopup="true" tabindex="-1" class="slicknav_item slicknav_row" style="outline: none;">
                                                <a href="#" tabindex="-1">Services <i class="icofont-rounded-down"></i></a>
                                                <span class="slicknav_arrow">►</span>
                                            </a>
                                            <ul class="dropdown slicknav_hidden" role="menu" aria-hidden="true" style="display: none;">
                                                <li><a href="service.html" role="menuitem" tabindex="-1">الأقسام</a></li>
                                                <li>
                                                    <a href="service-details.html" role="menuitem" tabindex="-1">Service Details</a>
                                                </li>
                                            </ul>
                                        </li>

                                        <li><a href="contact.html" role="menuitem" tabindex="-1">التواصل</a></li>
                                    </ul>
                                </div>
                            </div>
                            <!-- End Mobile Nav -->
                        </div>
                        <div class="col-lg-7 col-md-9 col-12">
                            <!-- Main Menu -->
                            <div class="main-menu">
                                <nav class="navigation">
                                    <ul class="nav menu">
                                        <li class="active">
                                            <a href="#">الرئيسية <i class="icofont-rounded-down"></i></a>

                                        </li>

                                        <li><a href="contact.html"></a></li>

                                        <li><a asp-controller="Trainer" asp-action="Index">المدربين</a></li>
                                        <li><a asp-controller="Account" asp-action="Login">تسجيل</a></li>

                                        <li class="">
                                            <a href="#">البيانات الاساسية <i class="icofont-rounded-down"></i></a>
                                            <ul class="dropdown">
                                                <li class="active">
                                                <li><a asp-controller="Qualification" asp-action="Index">المؤهلات</a></li>
                                                <li><a asp-controller="Specialization" asp-action="Index">التخصصات</a></li>
                                                <li><a asp-controller="Department" asp-action="Index">الأقسام</a></li>
                                                <li><a asp-controller="CourseClassification" asp-action="Index">تصنيفات الدورات</a></li>
                                            </ul>
                                        </li>
                                    </ul>
                                </nav>
                            </div>
                            <!--/ End Main Menu -->
                        </div>
                        <div class="col-lg-2 col-12">
                            <div class="get-quote">
                                <a href="appointment.html" class="btn"> عرض الدورات</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ End Header Inner -->

    </header>


    <div class="container pb-5 pt-5">
        @RenderBody()
    </div>
    <script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>

